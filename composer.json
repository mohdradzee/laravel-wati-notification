{
  "name": "mohdradzee/laravel-wati-notification",
  "description": "A custom Laravel notification channel for WATI WhatsApp API",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Radzee AR",
      "email": "radzee.ar@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "mohdradzee\\WatiNotification\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
        "mohdradzee\\WatiNotification\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "mohdradzee\\WatiNotification\\WatiServiceProvider"
      ]
    }
  },
  "require": {
    "php": "^8.1",
    "illuminate/notifications": "^10.0",
    "illuminate/support": "^10.0",
    "guzzlehttp/guzzle": "^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.36",
    "phpunit/phpunit": "^10.5"
  }
}
